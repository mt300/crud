<%- include("../../partials/header.ejs") %> 
<%- include("../../partials/homenavbar.ejs") %> 
<body>
    <div class="container">
        <hr>
        <% result.articles.rows.forEach(article => { %>
            <div class="card">
                <div class="card-header">
                    <h2><%= article.title %> </h2>
                </div>
                <dic class="card-body">
                    <a href="/<%= article.slug %> " class="btn btn-primary">Read it!</a>
                </dic>
            </div>
        <% }) %>
    </div>
</body>
